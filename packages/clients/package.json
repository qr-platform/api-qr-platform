{
  "name": "@qr-platform/api-qr-platform",
  "version": "1.0.0",
  "description": "API clients for QR Platform",
  "main": "dist/node/index.js",
  "types": "dist/node/index.d.ts",
  "files": [
    "dist",
    "node.js",
    "react.js",
    "node.d.ts",
    "react.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "default": "./dist/node/index.js"
    },
    "./node": {
      "types": "./node.d.ts",
      "default": "./node.js"
    },
    "./react": {
      "types": "./react.d.ts",
      "default": "./react.js"
    }
  },
  "scripts": {
    "build": "npm run build:node && npm run build:react && npm run build:entries",
    "build:node": "tsc -p tsconfig.node.json",
    "build:react": "tsc -p tsconfig.react.json",
    "build:entries": "node scripts/build-entries.js",
    "generate:node": "openapi-ts -i http://localhost:8787/v1/openapi -o ./src/node --plugins @hey-api/client-fetch @hey-api/sdk @hey-api/typescript",
    "generate:react": "openapi-ts -i https://api.qr-platform.com/v1/openapi -o ./src/react --plugins @tanstack/react-query @hey-api/typescript",
    "generate": "npm run generate:node && npm run generate:react",
    "clean": "rm -rf dist *.js *.d.ts && find src -name '*.js' -delete && find src -name '*.d.ts' -delete"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@7nohe/openapi-react-query-codegen": "^1.6.2",
    "@hey-api/openapi-ts": "^0.77.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "typescript": "^5.6.0"
  },
  "keywords": [
    "api-client",
    "qr-code",
    "nodejs",
    "react",
    "react-query",
    "typescript"
  ],
  "repository": "your-repo/@qr-platform/api-qr-platform",
  "license": "ISC",
  "author": ""
}
